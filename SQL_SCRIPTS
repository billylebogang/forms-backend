	"type_of_id" text COLLATE pg_catalog."default",
	"expiry_date" text COLLATE pg_catalog."default",
	"occupation" text COLLATE pg_catalog."default",
	"proffesion" text COLLATE pg_catalog."default",
	"physical_address" text COLLATE pg_catalog."default",
	"postal_address" text COLLATE pg_catalog."default",
	"msisdn" text COLLATE pg_catalog."default",
	"other_contact" text COLLATE pg_catalog."default",
	"other_contact1" text COLLATE pg_catalog."default",
	"next_of_kin" text COLLATE pg_catalog."default",
	"NOK_relationship" text COLLATE pg_catalog."default",
	"NOK_contact" text COLLATE pg_catalog."default",
	"cutomer_agent" text COLLATE pg_catalog."default",
	"agent_outlet" text COLLATE pg_catalog."default",
	"cutomer_agent_location" text COLLATE pg_catalog."default",
	"date_attented" text COLLATE pg_catalog."default",
	"form_id":


)

TABLESPACE pg_default;

